  border: 2rpx solid #85c610;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.container .common-container .wx-logined .wx-login .avatar-info .tip .wx-quick-login .text {
  width: 160rpx;
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #82c400;
  line-height: 28rpx;
}
.container .common-container .wx-logined .wx-login .logined-tip {
  width: 128rpx;
  height: 48rpx;
  border-radius: 10rpx;
  border: 2rpx solid #b8b8b8;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
}
.container .common-container .wx-logined .wx-login .logined-tip .text {
  width: 60rpx;
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #b8b8b8;
  line-height: 28rpx;
}
.container .common-container .wx-logined .wechat-phone {
  width: 690rpx;
  height: 100rpx;
  background: #07c160;
  border-radius: 20rpx;
  border: 2rpx solid #08954b;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.container .common-container .wx-logined .wechat-phone .text {
