}
.container .common-container .wx-logined .other-phone {
  width: 690rpx;
  height: 100rpx;
  background: #fbfafc;
  border-radius: 20rpx;
  border: 2rpx solid rgba(0, 0, 0, 0.15);
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-top: 28rpx;
}
.container .common-container .wx-logined .other-phone .text {
  color: #353535;
}
.container .common-container .dialog-container {
  display: flex;
  flex-direction: column ;
  justify-content: center;
  align-items: center;
}
.container .common-container .dialog-container .date-picker {
  width: 100%;
}
.container .common-container .dialog-container .selection {
  width: 100%;
  height: 224rpx;
  border-radius: 24rpx 24rpx 0rpx 0rpx;
}
.container .common-container .dialog-container .selection .option {
  width: 100%;
  height: 112rpx;
  background: #ffffff;
  display: flex;
  flex-direction: row ;
  justify-content: center;
  align-items: center;
}
.container .common-container .dialog-container .selection .option .text {
  width: auto;
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 48rpx;
}
.container .common-container .dialog-container .delete-cancel {
  width: 100%;
  height: 112rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-top: 16rpx;
}
.container .common-container .dialog-container .delete-cancel .delete-cancel-text {
  width: auto;
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 48rpx;
}
.container .common-container .dialog-container .split-bar {
  width: 268rpx;
  height: 10rpx;
  background-color: #000000;
  border-radius: 200rpx;
  margin: 40rpx 0rpx 18rpx 0rpx;
}
.container .van-picker__cancel {
  color: #969696 !important;
}
.container .van-picker__confirm {
  color: #f96d3d !important;
}
