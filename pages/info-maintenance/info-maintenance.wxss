  border-radius: 50%;
}
.container .common-container .wx-logined .wx-login .avatar-info .tip {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  margin-left: 34rpx;
}
.container .common-container .wx-logined .wx-login .avatar-info .tip .name {
  width: auto;
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 48rpx;
}
.container .common-container .wx-logined .wx-login .avatar-info .tip .wx-quick-login {
  width: 224rpx;
  height: 48rpx;
