.container {
  background-color: #f7f7f7;
}
.container .common-container {
  flex-grow: 1;
  padding-top: 32rpx;
}
.container .common-container .main-info .change-avatar {
  width: 748rpx;
  height: 192rpx;
  background: #ffffff;
  box-shadow: 0rpx 2rpx 0rpx 0rpx #e2e2e2;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 32rpx 28rpx 32rpx 32rpx ;
  border-bottom: 1rpx solid #E2E2E2;
}
.container .common-container .main-info .change-avatar .avatar-info {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: auto;
  height: inherit;
}
.container .common-container .main-info .change-avatar .avatar-info .avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}
.container .common-container .main-info .change-avatar .avatar-info .avatar .cover-image {
  width: inherit;
  height: inherit;
  border-radius: 50%;
}
.container .common-container .main-info .change-avatar .avatar-info .tip {
  width: 136rpx;
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 48rpx;
  margin-left: 32rpx;
}
.container .common-container .main-info .info-container {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-end;
}
.container .common-container .main-info .info-container .basic-info {
  width: 100%;
  height: 112rpx;
  background: #ffffff;
  display: flex;
  direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 32rpx 0rpx 32rpx 32rpx;
}
.container .common-container .main-info .info-container .basic-info .info-name {
  width: auto;
  height: 48rpx;
  color: rgba(0, 0, 0, 0.9);
}
.container .common-container .main-info .info-container .basic-info .info-detail {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .common-container .main-info .info-container .basic-info .info-detail .info {
  width: auto;
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 48rpx;
  margin-right: 20rpx;
  opacity: 0.5;
}
.container .common-container .main-info .info-container .basic-info .info-detail .right-icon {
  margin-right: 32rpx;
}
.container .common-container .main-info .info-container .split-line {
  width: 716rpx;
  height: 2rpx;
  background-color: rgba(0, 0, 0, 0.1);
}
.container .common-container .wx-logined .login-tip {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
  height: auto;
  padding-left: 32rpx;
}
.container .common-container .wx-logined .login-tip .text {
  width: 168rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 40rpx;
}
.container .common-container .wx-logined .wx-login {
  margin-top: 16rpx;
  width: 750rpx;
  height: 176rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 34rpx 30rpx 34rpx 34rpx;
  margin-bottom: 266rpx;
}
.container .common-container .wx-logined .wx-login .avatar-info {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: auto;
  height: inherit;
}
.container .common-container .wx-logined .wx-login .avatar-info .avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}
.container .common-container .wx-logined .wx-login .avatar-info .avatar .cover-image {
  width: inherit;
