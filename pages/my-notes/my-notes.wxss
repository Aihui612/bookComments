.container {
  background-color: #f7f7f7;
}
.container .notes-top {
  width: 100%;
  height: 176rpx;
  background-color: #f96d3d;
  padding: 24rpx 30rpx 40rpx 32rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .notes-top .notes-info {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .notes-top .notes-info .notes-dot {
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  background-color: #ffffff;
  margin: 0 20rpx 0 22rpx;
}
.container .notes-top .notes-info .notes-title {
  width: auto;
  height: 56rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #efeff4;
  line-height: 56rpx;
}
.container .notes-top .notes-info .notes-num {
  width: auto;
  height: 56rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #efeff4;
  line-height: 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
}
.container .notes-top .notes-icon {
  width: 93rpx;
  height: 112rpx;
  background-color: rgba(229, 228, 229, 0.09);
}
.container .notes-container {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  flex-grow: 1;
}
.container .notes-container .notes-main {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100vw;
}
.container .notes-container .notes-main .notebook-container {
  width: 100%;
  height: auto;
