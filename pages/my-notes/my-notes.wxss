}
.container .notes-container .notes-main .notebook-container .check-notebooks {
  width: 140rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #576b95;
  line-height: 40rpx;
}
.container .notes-container .notes-main .notebook-container .notebooks {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: auto;
  height: auto;
  flex-wrap: wrap;
  flex: 1;
}
.container .notes-container .notes-main .notebook-container .notebooks .all {
