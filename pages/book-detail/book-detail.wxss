.container {
  align-items: center;
  background-color: #ffffff;
}
.container .top-container {
  flex: 1;
}
.container .top-container .book-card {
  width: 688rpx;
  height: 288rpx;
  background: #f7f7f7;
  border-radius: 10rpx;
  margin: 32rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding: 16rpx;
}
.container .top-container .book-card .cover-image {
  width: 176rpx;
  height: 256rpx;
  margin-right: 32rpx;
}
.container .top-container .book-card .info-detail {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.container .top-container .book-card .info-detail .name {
  width: 272rpx;
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #000000;
  line-height: 48rpx;
}
.container .top-container .book-card .info-detail .commentsNum {
  width: 108rpx;
  height: 26rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #969696;
  line-height: 26rpx;
}
.container .top-container .book-card .info-detail .info-obj {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .top-container .book-card .info-detail .info-obj .spacing {
  letter-spacing: 20rpx;
}
.container .top-container .book-card .info-detail .info-obj .seven-spacing {
  letter-spacing: 7rpx;
}
.container .top-container .book-card .info-detail .info-obj .one-spacing {
  letter-spacing: 1rpx;
}
.container .top-container .book-card .info-detail .info-obj .info-key {
  width: 126rpx;
  height: 32rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 32rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .top-container .book-card .info-detail .info-obj .info-value {
  width: auto;
  height: 32rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #576b95;
  text-align: left;
  line-height: 32rpx;
}
.container .top-container .tabs-container {
  padding: 32rpx 30rpx 32rpx 32rpx;
  width: 688rpx;
  height: auto;
}
.container .top-container .tabs-container .tab-container {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 39rpx;
}
.container .top-container .tabs-container .tab-container .editor-recommend {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 45rpx;
  margin-top: 32rpx;
}
.container .top-container .no-more {
  width: 100%;
  height: 32rpx;
  font-size: 22rpx;
  display: flex;
  margin-top: 32rpx;
  margin-bottom: 48rpx;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #B8B8B8;
  line-height: 32rpx;
}
.container .top-container .foot {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.container .top-container .foot .copy-right {
  width: auto;
  height: 28rpx;
  font-size: 20rpx;
  font-family: STSongti-SC-Regular, STSongti-SC;
  font-weight: 400;
  color: #b2b2b2;
  line-height: 28rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
}
.container .bottom-contianer {
  position: relative;
  margin-bottom: 0px;
  width: 100%;
  height: auto;
}
.container .bottom-contianer .bottom-input {
  width: 100%;
  height: 98rpx;
  background: #ffffff;
  box-shadow: 0rpx 2rpx 0rpx 0rpx #e2e2e2;
  padding: 16rpx 32rpx 18rpx  30rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .bottom-contianer .bottom-input .input {
  width: 608rpx;
  height: 64rpx;
  background-color: #f7f7f7;
  border-radius: 10rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #b8b8b8;
}
.container .bottom-contianer .bottom-input .icon-input {
  width: 48rpx;
  height: 56rpx;
  margin-left: 32rpx;
}
.container .bottom-contianer .bottom-input .icon-input .icon {
  width: inherit;
  height: inherit;
}
.container .bottom-contianer .bottom-line {
  width: 100%;
  height: 68rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
}
.container .bottom-contianer .bottom-line .line {
  width: 268rpx;
  height: 10rpx;
  background: #000000;
  border-radius: 200rpx;
  margin: 0rpx 0rpx 18rpx 0rpx;
}
.container .cuIcon-people:before {
  content: "\e736";
}
.container .cuIcon-favorfill:before {
  content: "\e64b";
}
.container .cuIcon-right:before {
  content: "\e6a3";
}
.container .text-gray,
.container .line-gray,
.container .lines-gray {
  color: #aaaaaa;
}
.container .text-orange {
  color: #F96D3D;
}
.container .lg {
  width: 80upx;
  height: 80upx;
}
