.container {
  justify-content: flex-end;
}
.container .detail-container {
  flex-grow: 1;
  padding: 32rpx 30rpx 16rpx 32rpx;
  width: 750rpx;
  height: auto;
  background-color: #ffffff;
}
.container .detail-container .title {
  width: 688rpx;
  height: 78rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #0b0c0d;
  line-height: 39rpx;
}
.container .detail-container .type-info {
  width: 100%;
  height: 48rpx;
  border-radius: 24rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 48rpx;
}
.container .detail-container .type-info .info {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .detail-container .type-info .info .type {
  width: auto;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #969696;
  line-height: 34rpx;
  margin-right: 24rpx;
}
.container .detail-container .type-info .info .time {
  width: auto;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #969696;
  line-height: 34rpx;
}
.container .detail-container .type-info .opr-btn {
  width: 113rpx;
  height: 48rpx;
  background: #efeff4;
  border-radius: 24rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
}
.container .detail-container .type-info .opr-btn .cover-image {
  width: 20rpx;
  height: 22rpx;
}
.container .detail-container .type-info .opr-btn .text {
  width: 48rpx;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 34rpx;
}
.container .detail-container .comment {
  width: 688rpx;
  height: auto;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 39rpx;
}
.container .detail-container .basic-info {
  position: relative;
  width: 100%;
  height: auto;
  background-color: #ffffff;
  padding: 16rpx 16rpx 16rpx 16rpx;
  margin-top: 32rpx;
}
.container .detail-container .basic-info .book-shape {
  position: absolute;
  right: 16rpx;
  width: 16rpx;
  height: 24rpx;
  top: 4rpx;
}
.container .detail-container .basic-info .comment-container {
  width: 656rpx;
  height: 304rpx;
  background-color: #efeff4;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  padding: 16rpx;
}
.container .detail-container .basic-info .comment-container .top-container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.container .detail-container .basic-info .comment-container .top-container .userInfo {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .detail-container .basic-info .comment-container .top-container .userInfo .avatar {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  margin-right: 16rpx;
}
.container .detail-container .basic-info .comment-container .top-container .userInfo .avatar .cover-image {
  width: inherit;
  height: inherit;
  border-radius: 50%;
}
.container .detail-container .basic-info .comment-container .top-container .userInfo .info {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.container .detail-container .basic-info .comment-container .top-container .userInfo .info .name {
  width: auto;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #000000;
  line-height: 34rpx;
}
.container .detail-container .basic-info .comment-container .top-container .userInfo .info .time {
  width: auto;
  height: 26rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #969696;
  line-height: 26rpx;
}
.container .detail-container .basic-info .comment-container .top-container .book-info {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-end;
  width: auto;
}
.container .detail-container .basic-info .comment-container .top-container .book-info .book-name {
  width: auto;
  height: 34rpx;
  font-size: 24rpx;
  font-family: STSongti-SC-Bold, STSongti-SC;
  font-weight: bold;
  color: #666666;
  line-height: 34rpx;
}
.container .detail-container .basic-info .comment-container .top-container .book-info .scorer {
  width: auto;
  height: 26rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #969696;
  line-height: 26rpx;
}
.container .detail-container .basic-info .comment-container .content {
  width: 624rpx;
  height: 118rpx;
  font-size: 29rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 40rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 26rpx;
}
.container .detail-container .basic-info .comment-container .releation-info {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
}
.container .detail-container .basic-info .comment-container .releation-info .releation {
  width: auto;
  height: 32rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #969696;
  line-height: 32rpx;
}
.container .nomore-tip {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: auto;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #b8b8b8;
  line-height: 34rpx;
  margin: 46rpx 0rpx;
}
.container .bottom-btn {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.container .bottom-btn .btn-list {
  width: 750rpx;
  height: 98rpx;
  background-color: #ffffff;
  box-shadow: 0rpx 2rpx 0rpx 0rpx #e2e2e2;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .bottom-btn .btn-list .btn {
  width: 250rpx;
  height: 64rpx;
  box-shadow: -2rpx 0rpx 0rpx 0rpx #e2e2e2;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.container .bottom-btn .btn-list .btn .edit {
  width: 48rpx;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #f96d3d;
  line-height: 34rpx;
  margin-left: 20rpx;
}
.container .bottom-btn .btn-list .btn .share {
  width: 48rpx;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 34rpx;
  margin-left: 20rpx;
}
.container .bottom-btn .btn-list .btn .icon {
  width: 26rpx;
  height: 26rpx;
}
.container .bottom-btn .btn-bar {
  width: 750rpx;
  height: 68rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-end;
  padding-bottom: 18rpx;
}
.container .bottom-btn .btn-bar .bar {
  width: 268rpx;
  height: 10rpx;
  background: #000000;
  border-radius: 200rpx;
}
