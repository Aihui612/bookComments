  justify-content: center;
  align-items: center;
}
.comment-info .my-comment-card .content-container .basic-info .info-container .avatar .cover-image {
  width: inherit;
  height: inherit;
  border-radius: 50%;
}
.comment-info .my-comment-card .content-container .basic-info .info-container .info {
  margin-left: 16rpx;
}
.comment-info .my-comment-card .content-container .basic-info .info-container .info .name {
