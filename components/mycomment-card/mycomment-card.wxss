  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.comment-info .my-comment-card .content-container {
  width: 100%;
  height: inherit;
  border-bottom: 2rpx solid #EFEFF4;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.comment-info .my-comment-card .content-container .basic-info {
  width: inherit;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.comment-info .my-comment-card .content-container .basic-info .info-container {
